{
  "name": "monad",
  "version": "0.0.0",
  "description": "Generic AngularJS-based CMS framework",
  "main": "gulpfile.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "jspm": {
    "directories": {
      "lib": ".",
      "packages": "vendor"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.3.15",
      "angular-translate": "github:angular-translate/bower-angular-translate@^2.6.1",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.13",
      "bootstrap": "github:twbs/bootstrap@^3.3.4",
      "jquery": "github:components/jquery@^2.1.3"
    },
    "devDependencies": {
      "babel": "npm:babel@^4.7.16",
      "babel-runtime": "npm:babel-runtime@^4.7.16"
    }
  },
  "dependencies": {
    "babel": ">=4.7.12",
    "es6-module-loader": "*"
  },
  "devDependencies": {
    "gulp": ">=3.8.11",
    "gulp-compass": ">=2.0.3",
    "gulp-watch": ">=4.1.0",
    "jest-cli": ">=0.2.2",
    "jest": "*",
    "babel-jest": "*"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@dev.sensimedia.nl/home/git/libraries/monad"
  },
  "author": "Marijn Ophorst <marijn@monomelodies.nl>",
  "license": "MIT"
}
